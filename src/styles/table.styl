.table {
  font-size: 12px;
  text-align: center;

  @media (max-width: 992px) {
    font-size: .75rem;
  }
  @media (max-width: 768px) {
    font-size: .60rem;
  }
  @media (max-width: 400px) {
    font-size: .50rem
  }
}


table > .ant-table-thead > tr > th{
  text-align: center;
  // background-color: #FEF0E8;
  @media (max-width: 992px) {
    font-size: .75rem;
  }
  @media (max-width: 768px) {
    font-size: .60rem;
  }
  @media (max-width: 400px) {
    font-size: .50rem
  }
}

.low {
  background: #EEFAE5;
}

.moderate {
  background: #FDF2DD;
}

.high {
  background: #FCE2E1;
}

.ReactTable {
  font-family: Roboto;
  display: flex;
  max-width 100%;
  overflow-x: hidden;

  @media (max-width: 992px) {
    font-size: .7rem;
  }
  @media (max-width: 768px) {
    font-size: .6rem;
  }
  @media (max-width: 400px) {
    font-size: .5rem;
  }
}

.ReactTable .rt-tr {
  height: 3rem;
  font-size .7rem;
  display: flex;
  justify-content: center;
  align-items: center;
}
