.table {
  font-size: 12px;
  text-align: center;

  @media (max-width: 992px) {
    font-size: 12px;
  }
  @media (max-width: 768px) {
    font-size: 10px;
  }
  @media (max-width: 400px) {
    font-size: 9px
  }
}

table > .ant-table-thead > tr > th{
  text-align: center;
  // background-color: #FEF0E8;
  @media (max-width: 992px) {
    font-size: 12px;
  }
  @media (max-width: 768px) {
    font-size: 10px;
  }
  @media (max-width: 400px) {
    font-size: 8px
  }
}


